extends ../layouts/default
block content
    section
      #inner-banner
      #inner-content-box
        #content-box
          .container
            #offerings
              include ../views/icon.jade
              .col-md-9#content-data
                h2 #{data.pharmaceuticals[0].heading}
                .row
                  .col-md-4
                    if data.pharmaceuticals.length
                          img(id="image",src=data.pharmaceuticals[0].image.url)
                  .col-md-8
              include ../views/footermenunavigation