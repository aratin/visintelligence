extends ../layouts/default
block content
    section
      #inner-banner
      #inner-content-box
        #content-box
          .container
            #offerings
              include ../views/icon.jade
              .col-md-9#content-data
                h2 #{data.businessprocessstrategys[0].heading}
                .row
                  .col-md-4
                    if data.businessprocessstrategys.length
                        if data.businessprocessstrategys[0].image.exists
                          img(id="image",src=data.businessprocessstrategys[0].image.url)
                  .col-md-8
                    p
                      | #{data.businessprocessstrategys[0].longText}
              .expand
              include ../views/listingmenu
              