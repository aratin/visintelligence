extends ../layouts/default
block content
    section
      #inner-banner
      #inner-content-box
        #content-box
          .container
            #offerings
              include ../views/icon.jade
              .col-md-9#content-data
                h2 #{data.healthcareinsurers[0].heading}
                .row
                  .col-md-4
                    if data.healthcareinsurers.length
                          img(id="image",src=data.healthcareinsurers[0].image.url)
                  .col-md-8
                      p
                        | #{data.healthcareinsurers[0].longText}
                        br
                        | #{data.healthcareinsurers[0].longString}
              .expand 
              include ../views/footermenunavigation